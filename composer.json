{
  "name": "jbaron/sitemapcrawler",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension for parsing a sitemap and fetching its entries to warm up caches.",
  "license": [
    "MIT"
  ],
  "keywords": ["TYPO3 CMS", "Cache warmup", "sitemap"],
  "support": {
    "email": "jost.baron@mein-bauernhof.de"
  },
  "require": {
    "php": "^7.4 || ^8.0 || ^8.1",
    "typo3/cms-core": "^10.4 || ^11.5",
    "typo3/cms-extbase": "^10.4 || ^11.5",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Jbaron\\Sitemapcrawler\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "sitemapcrawler"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  }
}
